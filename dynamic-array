import java.util.Scanner;

class array
{
    public void arra()
{
    int a[]=new int[4];


    for(int i = 0; i<4; i++ )
    {
       Scanner sc =new Scanner(System.in);
       System.out.println("Enter a array number :");
       a[i]=sc.nextInt();
    }

    for(int i = 0; i<4; i++ )
    {
        System.out.print(a[i]);
    }


}
}
public class array1 {
    public static void main(String[] args)
    {
        array refv = new array();
        refv.arra();

    }
}
